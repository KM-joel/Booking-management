<?xml version="1.0" encoding="utf-8" ?>
<odoo>

     <record id="product_category_view_owl_tree_view" model="ir.ui.view">
        <field name="name">Product Categories</field>
        <field name="model">product.category</field>
        <field name="arch" type="xml">
            <owl_list />
        </field>
    </record>

    <record id='product_category_action_form' model='ir.actions.act_window'>
        <field name="name">Product Categories</field>
        <field name="res_model">product.category</field>
        <field name="view_mode">tree,owl_list,form</field>
    </record>

    <menuitem
    name="Category of article"
    id="article_cat"
    parent="booking_management.article_category"
    action="product_category_action_form"
  />

</odoo>
